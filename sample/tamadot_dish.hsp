#include "hsp3dish.as"

	tamadot=ginfo_newid
	celload dir_tv+"tamadot.png",tamadot
	tama_sx=64:tama_sy=64
	celdiv tamadot,tama_sx,tama_sy

	anim=0

*main
	redraw 0
	color 255,255,255:boxf

	gmode 2
	pos 0,0
	celput tamadot,38
	celput tamadot,38
	celput tamadot,38
	celput tamadot,38
	pos 0,0
	celput tamadot,anim+4
	celput tamadot,anim+4+8
	celput tamadot,anim+4+16
	celput tamadot,anim+4+24
	anim=anim+1
	if anim>=4 : anim=0

	redraw 1
	await 1000/5
	goto *main

